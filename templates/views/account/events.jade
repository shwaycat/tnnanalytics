extends ../../layouts/default

block content
	.row.content-row
		.col-xs-12
			h1.view-title=title
			a.btn.btn-default.analytics-cta(data-events-action="close-all") Mark all Closed
	.row
		.col-xs-12.events-container
			table#events-table.table.events-table
				thead
					tr
						th
						th Date
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
						th Channel
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
						th Status
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
						th ID
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
						th Last Accessed
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
						th.event-link-cell Link
							.span.entypo.entypo-chevron-up
							.span.entypo.entypo-chevron-down
				tbody