extends ../../layouts/default

block session
	.session-content
		
		a.login-logo(href="/" title="Cadence Home")
			img(src="/images/logo.png" alt="Cadence Logo")
		+flash-messages(messages)
		+auth-forms({ modal: false, initial: 'signin' })

block js
	script.
		jQuery(function($) {
			$('.auth-form').find('.form-control').eq(0).focus().select();
		});
