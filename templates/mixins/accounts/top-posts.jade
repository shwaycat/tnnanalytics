mixin topFacebookPost(options)
  .col-xs-12.col-md-12.col-lg-6.content-block.post-block.equal-height-block-1
    .inner-content
      h4.section-title(data-toggle="tooltip" data-trigger="click hover" data-placement="right" title=tooltip.topFacebookPost)
        =sectionTitles.topFacebookPost
      section.top-posts.top-facebook-post#topFacebookPost(data-admin-options=options)
        article.post

        section.post-details
          h3.post-title-details Details
          ul.post-details-list


