mixin analytics-section(type, options)

  if type == 'refTraffic'
    .col-xs-12.col-md-12.col-lg-6.content-block.donut-block.equal-height-block-2
      .inner-content
        h4.section-title(data-toggle="tooltip" data-trigger="click hover" data-placement="right" title=tooltip.refTraffic)
          =sectionTitles.refTraffic
        section.novo-graph.novo-donut-graph.reach#refTraffic(data-admin-options=options)
          
